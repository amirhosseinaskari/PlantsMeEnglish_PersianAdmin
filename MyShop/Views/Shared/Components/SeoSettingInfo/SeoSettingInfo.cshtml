@model SEOSettings
@if (Model != null)
{
    if (Model.IsBlock)
    {
        <meta name="robots" content="noindex">
    }
    else
    {
        
        
        if (!string.IsNullOrWhiteSpace(Model.GoogleSearchConsole))
        {
            <meta name="google-site-verification" content="@Model.GoogleSearchConsole">
        }
        if (!string.IsNullOrWhiteSpace(Model.GoogleAnalytics))
        {
            <!-- Google Analytics -->
            <script>
                (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

                ga('create', @Model.GoogleAnalytics, 'auto');
                ga('send', 'pageview');
            </script>
            <!-- End Google Analytics -->
        }

        if (!string.IsNullOrWhiteSpace(Model.FavIcon))
        {
            <link rel="apple-touch-icon" sizes="180x180" href="~/apple-touch-icon.png">
            <link rel="icon" type="image/png" sizes="32x32" href="~/favicon-32x32.png">
            <link rel="icon" type="image/png" sizes="16x16" href="~/favicon-16x16.png">
            <meta name="msapplication-TileColor" content="#da532c">
            <meta name="theme-color" content="#ffffff">
        }
        else
        {
            <link rel="icon" href="~/favicon.ico" />
        }
    }



}