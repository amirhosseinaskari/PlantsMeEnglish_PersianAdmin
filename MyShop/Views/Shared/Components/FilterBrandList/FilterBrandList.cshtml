@model IEnumerable<Brand>

@foreach (var item in Model)
{
    <li class="filter-subitem-list__list-item">
        <label>
            <input class="uk-checkbox" type="checkbox"
                   id="brandSelect@(item.BrandId)"
                   data-id="@item.BrandId" 
                   name="brandCheckbox">
             <span class="dana-font fontsize-14 mobile-fontsize-12 margin-right-10 font-weight-500">@item.Title</span>
        </label>
    </li>
}