@model IEnumerable<SubProductVariation>
@if (Model != null)
{
    @if (Model.Count() > 0)
    {
        <ul class="list-unstyled margin-0 padding-0">
            @foreach (var item in Model)
            {
                <li class="dana-font fontsize-14 d-flex align-items-start uk-text-right subpro-var-item
                justify-content-start mobile-fontsize-12 digit-format margin-y-5">
                    <div class="margin-x-10 font-weight-400">
                        <span>@item.ProductVariationTitle</span>
                        <span>:</span>
                    </div>
                    <div class="font-weight-500">
                        @item.Title
                    </div>
                </li>
            }
        </ul>
    }
}
